<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#003300"
    tools:context="niners.ninernav.NavigationScreen">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->

    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Navigation"
        android:textSize="60dp"
        android:textColor="#cccc00"
        android:layout_above="@+id/editText"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="72dp"
        android:id="@+id/textView4" />

    <Button
        android:text="Go"
        android:textSize="25dp"
        android:background="#cccc00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="71dp"
        android:id="@+id/button6"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <AutoCompleteTextView
        android:text="Search Building or My Location"
        android:layout_width="275dp"
        android:layout_height="40dp"
        android:id="@+id/autoCompleteTextView2"
        android:layout_marginTop="163dp"
        android:background="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp" />

    <AutoCompleteTextView
        android:text="Search Building"
        android:layout_width="275dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:layout_marginTop="92dp"
        android:id="@+id/autoCompleteTextView3"
        android:layout_below="@+id/autoCompleteTextView2"
        android:layout_alignLeft="@+id/autoCompleteTextView2"
        android:layout_alignStart="@+id/autoCompleteTextView2" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="To:"
        android:textSize="26dp"
        android:textColor="#cccc00"
        android:ems="10"
        android:id="@+id/textView5"
        android:layout_alignBaseline="@+id/autoCompleteTextView3"
        android:layout_alignBottom="@+id/autoCompleteTextView3"
        android:layout_toLeftOf="@+id/autoCompleteTextView3"
        android:layout_toStartOf="@+id/autoCompleteTextView3" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="From:"
        android:textSize="26dp"
        android:textColor="#cccc00"
        android:ems="10"
        android:id="@+id/editText"
        android:layout_alignTop="@+id/autoCompleteTextView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp" />

</RelativeLayout>
